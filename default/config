blog+=(
	title        'My Blog'
	tagline      'Just another Zsh blog'
	theme        'default'
	root         '/'
	archives     '/archives'
	posts        5
	recent-posts 10
)

publish+=(
	transport 'ftp'

	user      "$USER"
	pass      ''
	host      ''
	root      '~/public_html'
)

links+=(
	# 'Link text' 'http://link.url/'
	'Gittip' 'https://www.gittip.com/rummik/'
	'Github' 'https://github.com/rummik'
)

